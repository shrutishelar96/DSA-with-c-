
targetsum -



#include <iostream>
#include<vector>
using namespace std;

int main() {
   
    
    vector<int> v;
    v.push_back(1);
    cout<<"size: "<<v.size()<<endl;
    cout<<"capacity"<<v.capacity()<<endl;
    v.push_back(2);
    cout<<"size: "<<v.size()<<endl;
    cout<<"capacity"<<v.capacity();
    

    return 0;
}


//output-


size: 1
capacity1
size: 2
capacity2

