targetsum of array-


#include <iostream>
#include<vector>
using namespace std;

int main() {
    // Write C++ code here
    
   int array[6]={3,1,2,2,3,4};
   int targetsum=4;
   int size=6;
   int pairs=0;
   for(int i=0;i<size;i++){
       for(int j=i+1;i<size;i++){
           if(array[i]+array[j]==targetsum){
               pairs++;
           }
       }
   }
   cout<<pairs<<endl;

    return 0;
}

//output-
2

