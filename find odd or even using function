//find odd or even using function-

#include <iostream>
using namespace std;
bool isodd(int num){
    if(num % 2==0)
    {
        return false;
    }
    else{
        return true;
    }
}

int main() {
    int num;
    cin>> num;
    if(isodd(num)){
        cout<<num<<" odd ";
    }
else{
         cout<<"even";
    return 0;
}

