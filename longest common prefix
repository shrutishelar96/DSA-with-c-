 // longest common prefix-                    

#include <iostream>
#include<vector>
#include<string.h>
#include<algorithm>

using namespace std;
string Lcp(vector<string>&str){
    //sorting of array
    sort(str.begin(),str.end());
    string s1 =str[0];
    int i=0;
    string s2= str[str.size()-1];
    int j=0;
    
    string ans=" ";
    while(i<s1.size()&& j<s2.size()){
        if(s1[i]==s2[j]){
            ans+=s1[i];
            i++;j++;
        }
        else{
            break;
        }
    }
    return ans;
}

int main()
{
    int n;
    cout<<"entre no of string";
    cin>>n;
    
    cout<<"Entre string  : ";
    vector<string> str(n);
    for(int i=0;i<n;i++){
        cin>>str[i];
    }
   cout<<"longest common prefix :"<<Lcp(str)<<endl;
    return 0;
}


// output-
entre no of string 3
Entre string  : ram
raman
ramdev
longest common prefix : ram



